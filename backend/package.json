{
  "name": "backend",
  "version": "1.0.0",
  "description": "Dottie is a user-friendly application designed to help individuals understand their menstrual health better. Through a series of simple questions, Dottie collects information about your menstrual cycle and provides personalized analysis and recommendations.",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "npm i && node scripts/setup-env.js && nodemon server.js",
    "db:init": "node db/init.js",
    "db:init:sqlite": "node db/init-sqlite.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run -t unit",
    "test:dev": "cross-env TEST_MODE=true vitest run -t dev",
    "test:dev:watch": "cross-env TEST_MODE=true vitest -t dev",
    "test:prod": "vitest run -t prod",
    "test:prod:watch": "vitest -t prod",
    "test:pattern": "vitest run -t",
    "vercel-build": "node vercel-build.js",
    "___test workflow____": "___test workflow using vitest and test driven development____",
    "___STATUS:PASSED____": "___the following tests have passed____",
    "test:setup:unit": "vitest run routes/setup/__tests__/unit",
    "test:setup:dev": "cross-env TEST_MODE=true vitest run routes/setup/__tests__/e2e/dev",
    "test:setup:prod:real": "cross-env USE_MOCKS=false vitest run routes/setup/__tests__/e2e/prod",
    "test:auth": "vitest run routes/auth/__tests__",
    "test:auth:unit": "vitest run routes/auth/__tests__/unit",
    "test:auth:dev": "cross-env TEST_MODE=true vitest run routes/auth/__tests__/e2e/dev",
    "test:auth:dev:success": "cross-env TEST_MODE=true vitest run routes/auth/__tests__/e2e/dev/success",
    "test:auth:dev:error": "cross-env TEST_MODE=true vitest run routes/auth/__tests__/e2e/dev/error",
    "___STATUS:TODO____": "___the following tests need to be verified____",
    "test:assessment:unit": "vitest run routes/assessment/__tests__/unit",
    "test:assessment:dev": "cross-env TEST_MODE=true vitest run routes/assessment/__tests__/e2e/dev",
    "test:setup:prod": "vitest run routes/setup/__tests__/e2e/prod",
    "test:auth:prod:success": "vitest run routes/auth/__tests__/e2e/prod/success",
    "test:auth:prod:error": "vitest run routes/auth/__tests__/e2e/prod/error",
    "test:auth:prod": "vitest run routes/auth/__tests__/e2e/prod",
    "test:assessment:prod": "vitest run routes/assessment/__tests__/e2e/prod",
    "test:assessment:prod:success": "vitest run routes/assessment/__tests__/e2e/prod/success",
    "test:assessment:prod:error": "vitest run routes/assessment/__tests__/e2e/prod/error",
    "test:assessment": "vitest run routes/assessment/__tests__",
    "test:api": "playwright test",
    "test:api:setup": "playwright test -g \"setup\"",
    "test:api:basic": "playwright test -g \"Basic API\"",
    "test:api:auth": "playwright test -g \"Authentication\"",
    "test:api:assessment": "playwright test -g \"Assessment\"",
    "test:api:user": "playwright test -g \"User\"",
    "test:api:report": "playwright show-report",
    "test:api:ordered": "npm run test:api:setup && npm run test:api:basic && npm run test:api:auth && npm run test:api:assessment && npm run test:api:user",
    "test:playwright": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=playwright-results.xml playwright test",
    "deploy:vercel": "vercel deploy --prod",
    "supabase:init": "node db/supabase/scripts/init-supabase.js",
    "migrate:to-supabase": "node db/supabase/scripts/migrateToSupabase.js",
    "cleanup:azure": "node db/supabase/scripts/cleanupAzure.js",
    "test:supabase": "node db/supabase/scripts/test-connection.js",
    "check:tables": "node db/supabase/scripts/check-tables.js",
    "initialize:db": "node db/supabase/scripts/initialize-db.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^0.7.0",
    "@google/generative-ai": "^0.24.0",
    "@supabase/supabase-js": "^2.49.4",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mssql": "^10.0.2",
    "node-fetch": "^3.3.2",
    "sqlite3": "^5.1.7",
    "tedious": "^16.7.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0",
    "vitest": "^3.0.8"
  }
}
